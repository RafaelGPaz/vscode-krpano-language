{
    "name": "vscode-krpano",
    "displayName": "vscode-krpano",
    "description": "An extension for VS Code which provides support for the Krpano language.",
    "version": "0.0.1",
    "publisher": "RafaelGPaz",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xml",
            "aliases": ["Krpano Markup Language", "xml"],
            "extensions": [".xml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "xml",
            "scopeName": "source.xml.kml",
            "path": "./syntaxes/kml.tmLanguage"
        }]
    }
}